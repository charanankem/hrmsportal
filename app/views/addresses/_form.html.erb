<%= form_with model:@address do  |f| %>
  <div class="field-form">
      <%= f.label :door_no %><br />
      <%= f.text_field :door_no, autofocus: true, autocomplete: "door_no" %>
  </div>

  <div class="field-form">
    <%= f.label :street %><br />
    <%= f.text_field :street, autofocus: true, autocomplete: "street" %>
  </div>


  <div class="field-form">
    <%= f.label :city %><br />
    <%= f.text_field :city, autofocus: true, autocomplete: "city" %>
  </div>

  <div class="field-form">
  <%= f.label :state %><br />
  <%= f.text_field :state, autofocus: true, autocomplete: "state" %>
  </div>

  <div class="field-form">
  <%= f.label :pincode %><br />
  <%= f.text_field :pincode, autofocus: true, autocomplete: "pincode" %>
  </div>

  <div class="field-form">
  <%= f.label :country %><br />
  <%= f.text_field :country, autofocus: true, autocomplete: "country" %>

  <div class="field-form">
  <%= f.label :employee_id %><br />
  <%= f.text_field :employee_id, autofocus: true, autocomplete: "employee_id" %>
  </div>
  </div>



<div class="actions">
  <%= f.submit "Save",class: "save-button" %>
</div> 
<% end %>
